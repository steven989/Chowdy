
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
    <h4 class="modal-title" id="myModalLabel">Nutritional information</h4>
  </div>
  <%= form_for @nutritional_info, url: update_nutritional_info_path(@menu_item), method: :post do |f| %>
  <div class="modal-body">
      <div class="form-group">
          <%= f.label :protein %>
          <%= f.text_field :protein, class: "form-control" %>
      </div>
      <div class="form-group">
          <%= f.label :carb %>
          <%= f.text_field :carb, class: "form-control" %>
      </div>
      <div class="form-group">
          <%= f.label :fat %>
          <%= f.text_field :fat, class: "form-control" %>
      </div>
      <div class="form-group">
          <%= f.label :calories %>
          <%= f.text_field :calories, class: "form-control" %>
      </div>
      <div class="form-group">
          <%= f.label :fiber %>
          <%= f.text_field :fiber, class: "form-control" %>
      </div>
      <div class="form-group">
          <%= f.label :allergen %>
          <%= f.text_field :allergen, class: "form-control" %>
      </div>
      <div class="form-group">
          <%= f.label :spicy %>
          <%= f.check_box :spicy, class: "form-control" %>
      </div>
  </div>
  <div class="modal-footer">
    <%= f.submit class: "btn btn-md btn-default m-t-sm submit_data" %>
    <button type="button" class="btn btn-md btn-default m-t-sm" data-dismiss="modal">Close</button>
  </div>
  <% end %>