                                  <div class="col-sm-12">
                                    <div class="alert alert-<%= flash[:status] == "success" ? "success" : "danger" %>" style="margin-bottom:0 !important;">
                                      <%= flash[:notice_promotions] %>
                                    </div>
                                  </div>
                                </div>
                              <% end %>
                              <section class="row padder">
                                <div class="panel panel-default">
                                  <header class="panel-heading">
                                    Manage promotions
                                    <button class="btn btn-s-md btn-info btn-rounded pull_system_setting_form" href="<%= new_promotion_path %>">Create a new promotion</button>
                                  </header>
                                  <div class="table-responsive">
                                    <table id="system_setting" class="display table table-striped m-b-none" style="font-size: 12px">
                                      <thead>
                                        <tr>
                                          <th>ID</th>
                                          <th>Active?</th>
                                          <th>Amount (in cents)</th>
                                          <th>Date Promo Started</th>
                                          <th>Date Promo Ended</th>
                                          <th>Redeption Code</th>
                                          <th>Stripe Coupon ID</th>
                                          <th>Immediate Refund?</th>
                                          <th>New Customer Only?</th>
                                          <th>Number of Redeptions</th>
                                          <th>Edit</th>
                                          <th>Start/Stop Promotion</th>
                                          <th>Delete</th>
                                        </tr>
                                      </thead>
                                      <tbody>
                                        <% @promotions.each do |p| %>
                                          <tr>
                                            <td><%= p.id %></td>
                                            <td><%= p.active %></td>
                                            <td><%= p.amount_in_cents %></td>
                                            <td><%= p.start_date.strftime("%Y-%m-%d") unless p.start_date.nil? %></td>
                                            <td><%= p.end_date.strftime("%Y-%m-%d") unless p.end_date.nil? %></td>
                                            <td><%= p.code %></td>
                                            <td><%= p.stripe_coupon_id %></td>
                                            <td><%= p.immediate_refund %></td>
                                            <td><%= p.new_customer_only %></td>
                                            <td><%= p.promotion_redemptions.length %></td>
                                            <td><button class="btn btn-s-md btn-warning btn-rounded pull_system_setting_form" href="<%= edit_promotion_path(p) %>" style="min-width: 0px;width: 60px;">Edit</button></td>
                                            <% @coupon_activate_button = p.active ? "Stop" : "Start" %>
                                            <td><%= link_to @coupon_activate_button, activate_promotion_path(p), method: :put, class: "btn btn-s-md btn-warning btn-rounded", style:"min-width: 0px;width: 60px;" %></td>
                                            <td><%= link_to "Delete", promotion_path(p), method: :delete, class: "btn btn-s-md btn-danger btn-rounded", style: "min-width: 0px;width: 60px;" %></td>
                                          </tr>
                                        <% end %>
                                      </tbody>
                                    </table>
                                  </div>
                                </div> 
                              </section>
                            </section>
                        </section>
                    </section> 
                    <section class="tab-pane" id="system_settings" role="tabpanel">
                        <section class="vbox">
                            <section class="scrollable padder padder-v">
                              <% if flash[:notice_system_settings] %>
                                <div class="row m-t-sm">
                                  <div class="col-sm-12">
                                    <div class="alert alert-<%= flash[:status] == "success" ? "success" : "danger" %>" style="margin-bottom:0 !important;">
                                      <%= flash[:notice_system_settings] %>
                                    </div>
                                  </div>
                                </div>
                              <% end %>
                              <section class="row padder">
                                <div class="panel panel-default">
                                  <header class="panel-heading">
                                    System Settings
                                    <button class="btn btn-s-md btn-info btn-rounded pull_system_setting_form" href="<%= new_system_setting_path %>">Create new</button>
                                    <button class="btn btn-s-md btn-info btn-rounded pull_system_setting_form" href="<%= new_announcement_path %>">Create announcement</button>
                                    <button class="btn btn-s-md btn-default btn-rounded pull_system_setting_form" href="<%= edit_start_date_path %>">Change sign-up date (currently <%= StartDate.first.start_date.strftime("%Y-%m-%d") %>)</button>
                                  </header>
                                  <div class="table-responsive">
                                    <table id="system_setting_table" class="display table table-striped m-b-none" style="font-size: 12px">
                                      <thead>
                                        <tr>
                                          <th>ID</th>
                                          <th>Setting</th>
                                          <th>Setting Attribute</th>
                                          <th>Setting Value</th>
                                          <th>Edit</th>
                                          <th>Delete</th>
                                        </tr>
                                      </thead>
                                      <tbody>
                                        <% @system_settings.each do |s| %>
                                          <tr>
                                            <td><%= s.id %></td>
                                            <td><%= s.setting %></td>
                                            <td><%= s.setting_attribute %></td>
                                            <td><%=h truncate(s.setting_value, length: 100) %></td>
                                            <td><button class="btn btn-s-md btn-warning btn-rounded pull_system_setting_form" href="<%= edit_system_setting_path(s) %>">Edit</button></td>
                                            <td><%= link_to "Delete", system_setting_path(s), method: :delete, class: "btn btn-s-md btn-danger btn-rounded" %></td>
                                          </tr>
                                        <% end %>
                                      </tbody>
                                    </table>
                                  </div>
                                </div> 
                              </section>

                              <section class="row padder">
                                <div class="panel panel-default">
                                  <header class="panel-heading">
                                    Scheduled tasks
                                    <button class="btn btn-s-md btn-info btn-rounded pull_system_setting_form" href="<%= new_scheduled_task_path %>">Schedule new task</button>
                                  </header>
                                  <div class="table-responsive">
                                    <table id="scheduled_tasks" class="display table table-striped m-b-none" style="font-size: 12px">
                                      <thead>
                                        <tr>
                                          <th>ID</th>
                                          <th>Task name</th>
                                          <th>Day of week</th>
                                          <th>Hour of day</th>
                                          <th>Parameter 1</th>
                                          <th>Parameter 1 Type</th>
                                          <th>Parameter 2</th>
                                          <th>Parameter 2 Type</th>
                                          <th>Parameter 3</th>
                                          <th>Parameter 3 Type</th>
                                          <th>Last attempted run</th>
                                          <th>Last successful run</th>
                                          <th>Run now</th>
                                          <th>Edit</th>
                                          <th>Delete</th>
                                        </tr>
                                      </thead>
                                      <tbody>
                                        <% @scheduled_tasks.each do |t| %>
                                          <tr>
                                            <td><%= t.id %></td>
                                            <td><%= t.task_name %></td>
                                            <td><%= t.day_of_week %></td>
                                            <td><%= t.hour_of_day %></td>
                                            <td><%= t.parameter_1 %></td>
                                            <td><%= t.parameter_1_type %></td>
                                            <td><%= t.parameter_2 %></td>
                                            <td><%= t.parameter_2_type %></td>
                                            <td><%= t.parameter_3 %></td>
                                            <td><%= t.parameter_3_type %></td>
                                            <td><%= t.last_attempt_date %></td>
                                            <td><%= t.last_successful_run %></td>
                                            <td><a class="btn btn-s-md btn-warning btn-rounded" href="<%= run_task_path(t) %>">Run now</a></td>
                                            <td><button class="btn btn-s-md btn-warning btn-rounded pull_system_setting_form" href="<%= edit_scheduled_task_path(t) %>">Edit</button></td>
                                            <td><%= link_to "Delete", scheduled_task_path(t), method: :delete, class: "btn btn-s-md btn-danger btn-rounded" %></td>
                                          </tr>
                                        <% end %>
                                      </tbody>
                                    </table>
                                  </div>
                                </div> 
                              </section>

                            </section>                            
                        </section>

                    </section> 
                </section>
            </section>
        
        </section>
    </section>
</section>

