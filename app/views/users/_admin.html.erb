    bgColor:"#f2f4f8", //Hex, RGB or RGBA colors  
});
$(function(){
    $("#fakeLoader").fadeOut();
    //URL segment

      //retrieve
      var hash = (window.location.hash == "" || window.location.hash == "#_=_") ? "#dashboard" : window.location.hash;
      $('#main-nav li a[href='+hash+']').tab("show");
      $(".nav-main").find(".highlight").removeClass("highlight");
      $('#main-nav li a[href='+hash+']').parent().addClass("highlight");

      //navigation amongst the tabs
      $('#main-nav li a, .link_to_settings, #logo').off("click").on("click",function(){
        window.location.hash = $(this).attr("href");
        $(".nav-main").find(".highlight").removeClass("highlight");
        $(".nav-main").find('a[href="'+$(this).attr("href")+'"]').parent().addClass("highlight");
      });

      // ajax

      function formRetrievalAjax() {
          $('.pull_system_setting_form, .pull_next_week_total, .nestedForm').off("click").on("click",function(){
            event.preventDefault();
            $.ajax({
              url: $(this).attr('href'),
              type: 'GET',
              dataType: 'html'
            }).done(function(data){
              $('#formModal .modal-content').html(data);
              $('#formModal').modal({show:true});
              $("#formModal").animate({
                  scrollTop: 0
              }, 500);

              formRetrievalAjax();
            });
          });
      };

      // data tables

       $('#missing_hub_info').DataTable({
        searching: false,
        fnDrawCallback: function(oSettings){
          formRetrievalAjax();
        }
       });

       $('#system_setting_table').DataTable({
        iDisplayLength: 25
       });

       $('#feedback').DataTable({
        searching: false,
        order: [[1, "desc"]]
       });

       $('#overdue_invoice').DataTable({
        scrollX: true,
        searching: false,
        order: [[5, "asc"]]
       });

       $('#all_customers').DataTable({
        iDisplayLength: 10,
        scrollX: true,
        scrollY: Math.max(document.documentElement.clientHeight, window.innerHeight || 0) - 300,
        fnDrawCallback: function(oSettings){
          formRetrievalAjax();
        }
       });

       $('#scheduled_tasks').DataTable({
        searching: false,
        scrollX: true,
        paging: false
       });


      //collapse

      $('.meal_details_collapse_control').off("click").on("click",function(){
        if ($('.meals_details').hasClass("hidden")) {
          $('.meals_details').removeClass("hidden");
        } else {
          $('.meals_details').addClass("hidden");
        } 
      });

      // Plot sign ups and cancels
      // var sign_ups = {
      //   label: "Sign up",
      //   data: <%= @signup_timeseries %>

      // }
      // var cancels = {
      //   label: "Cancel",
      //   data: <%= @cancel_timeseries %>
      // }

      // var options = {
      //     series: {
      //         lines: {
      //             show: true,
